# ============================================================================
# CAREFLOW PULSE - CLOUD SCHEDULER CONFIGURATION (EXAMPLE)
# ============================================================================
# Configuration template for Cloud Scheduler Terraform variables.
# 
# Usage:
#   cp env.tfvars.example env.tfvars
#   # Update values in env.tfvars
#   terraform apply -var-file=vars/env.tfvars
# ============================================================================

# ============================================================================
# PROJECT CONFIGURATION
# ============================================================================

prod_project_id    = "your-production-project-id"
staging_project_id = "your-staging-project-id"
project_name       = "careflow"

# ============================================================================
# DEPLOYMENT CONFIGURATION
# ============================================================================

region = "us-central1"

pulse_agent_urls = {
  prod    = "https://your-careflow-agent-production.run.app"
  staging = "https://your-careflow-agent-staging.run.app"
}

pulse_agent_service_name = "careflow-agent"
